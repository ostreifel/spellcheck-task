{
  "manifestVersion": 1,
  "id": "spellcheck-task",
  "version": "0.1.3",
  "name": "Spellcheck Task",
  "scopes": [],
  "description": "Spellcheck specified files",
  "publisher": "ottostreifel",
  "galleryFlags": [
    "Preview"
  ],
  "icons": {
    "default": "static/images/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "demands": [
    "api-version/3.0"
  ],
  "tags": [
    "Spellcheck",
    "Sample"
  ],
  "categories": [
    "Build and release"
  ],
  "screenshots": [
    {
      "path": "static/images/screen1.png"
    }
  ],
  "content": {
    "license": {
      "path": "license.md"
    },
    "details": {
      "path": "overview.md"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/ostreifel/spellcheck-task"
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "files": [
    {
      "path": "spellcheck"
    }
  ],
  "contributions": [
    {
      "id": "spellcheck",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "spellcheck"
      }
    }
  ]
}