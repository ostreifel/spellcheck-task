{
  "manifestVersion": 1,
  "id": "spellcheck-task",
  "version": "1.0.6",
  "name": "Spellcheck Task",
  "scopes": [],
  "description": "Spellcheck specified files",
  "publisher": "ottostreifel",
  "public": true,
  "icons": {
    "default": "static/images/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "demands": [
    "api-version/3.0"
  ],
  "tags": [
    "Spellcheck",
    "spelling",
    "windows",
    "linux",
    "test"
  ],
  "categories": [
    "Build and release"
  ],
  "content": {
    "license": {
      "path": "license.md"
    },  
    "details": {
      "path": "overview.md"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/ostreifel/spellcheck-task"
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "files": [
    {
      "path": "spellcheck"
    },
    {
        "path": "static",
        "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "spellcheck",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "spellcheck"
      }
    }
  ]
}